{
  "name": "dicom-parser",
  "version": "2.0.2",
  "description": "JavaScript parser for DICOM Part 10 data",
  "homepage": "https://github.com/OHIF/dicom-parser",
  "author": "Chris Hafey <chafey@gmail.com> (https://github.com/chafey)",
  "contributors": [],
  "keywords": [
    "DICOM",
    "parse",
    "medical",
    "imaging"
  ],
  "main": "./dist/dicom-parser.min.js",
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OHIF/dicom-parser.git"
  },
  "bugs": {
    "url": "https://github.com/OHIF/dicom-parser/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "latest",
    "babelify": "^7.3.0",
    "benchmark": "latest",
    "chai": "latest",
    "coveralls": "latest",
    "es6-promise": "latest",
    "esperanto": "latest",
    "grunt": "~0.4",
    "grunt-benchmark": "latest",
    "grunt-cli": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-concat": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-env": "latest",
    "grunt-exec": "latest",
    "grunt-jscs": "latest",
    "grunt-karma": "latest",
    "grunt-nuget": "latest",
    "grunt-string-replace": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-firefox-launcher": "latest",
    "karma-qunit": "latest",
    "karma-sauce-launcher": "latest",
    "load-grunt-tasks": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "qunit": "^0.7.5",
    "qunit-cli": "^0.1.4",
    "spacejam": "latest",
    "uglify-js": "latest"
  },
  "ender": "./ender.js",
  "dojoBuild": "package.js",
  "jspm": {
    "files": [
      "dicom-parser.js",
      "locale"
    ],
    "map": {
      "dicom-parser": "./dicom-parser"
    },
    "buildConfig": {
      "uglify": true
    }
  },
  "scripts": {
    "lint": "eslint src/**.js test/**.js",
    "test": "npm run lint && mocha --require babel-register",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "browserify": "browserify -d src/dicom-parser.js -o dist/dicom-parser.js -p licensify",
    "minify": "uglifyjs dist/dicom-parser.js -c \"evaluate=false\" --source-map dist/dicom-parser.min.map --source-map-url dicom-parser.min.map -p relative -o dist/dicom-parser.min.js --comments --screw-ie8 -m",
    "build": "npm run browserify && npm run minify"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "spm": {
    "main": "dicom-parser.js",
    "output": [
      "locale/*.js"
    ]
  }
}
